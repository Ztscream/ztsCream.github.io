&lt;style&gt;
    .archives-list {
        margin: 20px 0;
    }

    .archive-year {
        margin-bottom: 30px;
    }

    .year-title {
        font-size: 1.5em;
        color: var(--primary);
        border-bottom: 2px solid var(--border);
        padding-bottom: 8px;
        margin-bottom: 15px;
    }

    .year-posts {
        display: flex;
        flex-direction: column;
        gap: 8px;
    }

    .archive-item {
        display: flex;
        align-items: center;
        gap: 15px;
        padding: 8px 0;
